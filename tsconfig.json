{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "skipLibCheck": true,
    "paths": {}
  },
  "include": ["**/*", ". babelrc"],
  "exclude": ["node_modules", "dist", "__tests__"]
}
